<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon_image_squarecopy"
        android:label="findeR"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/round_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.InternetSearch"
        tools:targetApi="31">
        <activity
            android:name=".listview_of_links"
            android:exported="false"
            android:label="@string/title_activity_listview_of_links"
            android:theme="@style/Theme.InternetSearch.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".TestHomeFragment"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".location_page"
            android:exported="false"
            android:label="@string/title_activity_location_page"
            android:theme="@style/Theme.InternetSearch.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".SplashScreen"
            android:exported="true"
            android:theme="@style/SplashScreen" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".signup_page"
            android:exported="true" />
        <activity
            android:name=".login_page"
            android:exported="true" />
        <activity
            android:name=".search_page"
            android:exported="true" />
    </application>

</manifest>